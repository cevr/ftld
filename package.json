{
  "name": "ftld",
  "private": false,
  "version": "0.59.1",
  "description": "a small, focused, library that provides a set of functional primitives for building robust and resilient applications in TypeScript",
  "license": "MIT",
  "author": "Cristian Ramos <hello@cvr.im>",
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup && cp dist/index.d.ts dist/index.d.mts",
    "prepare": "concurrently \"npm run test\" \"npm run build\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage && open coverage/index.html",
    "release": "changeset publish",
    "version": "changeset version"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@vitest/coverage-c8": "^0.31.1",
    "concurrently": "^8.1.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vitest": "^0.31.1",
    "zod": "^3.21.4"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "ftld",
    "fp",
    "functional",
    "functional programming",
    "small",
    "tiny",
    "utility",
    "utilities",
    "library"
  ],
  "homepage": "https://github.com/cevr/ftld",
  "repository": "github:cevr/ftld",
  "bugs": "https://github.com/cevr/ftld/issues"
}
